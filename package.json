{
  "name": "@txo/live-activity-countdown-react-native",
  "version": "1.0.11",
  "description": "Technology Studio - Live activity countdown React Native",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/live-activity-countdown-react-native.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "homepage": "https://github.com/technology-studio/react-native-live-activity-countdown#readme",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "build": "yarn build:clean && yarn build:lib",
    "clean": "del-cli example/android/build example/android/app/build example/ios/build",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "example": "yarn --cwd example",
    "lint:ci": "yarn eslint -c eslint-ci.config.js --cache . && yarn txo-eslint ccr --cache",
    "lint:fix": "eslint . --fix",
    "lint": "eslint --max-warnings 0 .",
    "prepare": "husky && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint:ci && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "test:watch": "concurrently \"yarn build:watch\" \"yarn test --watch\"",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "keywords": [
    "react-native",
    "ios"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@txo/live-activity-countdown-view-react-native": "^1.1.0"
  },
  "devDependencies": {
    "@txo-peer-dep/log": "^4.0.4",
    "@txo/commitlint": "^1.0.24",
    "@txo/functional": "^4.6.19",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^2.0.10",
    "@txo/tsconfig-cjs": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "@types/react": "~18.3.21",
    "@types/react-native": "0.73.0",
    "concurrently": "^9.1.2",
    "eslint-config-txo-package-react": "^1.0.26",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "react": "18.3.1",
    "react-native": "0.79.2",
    "redux-saga": "^1.3.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
